<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Current Seconds: <%= seconds %></p>

    <% if (seconds % 2 === 0) { %>
    <p>Even</p>
    <% } else { %>
    <ul>
      <% items.array.forEach(element => { %>
      <li><%= element %></li>

      <% }) %>
    </ul>
    <% } %>
    <%- htmlContent %>
  </body>
</html>
